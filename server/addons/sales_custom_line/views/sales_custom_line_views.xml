<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <record id="view_order_form_inherit_profit_percent" model="ir.ui.view">
        <field name="name">sale.order.form.inherit.profit.percent</field>
        <field name="model">sale.order</field>
        <field name="inherit_id" ref="sale.view_order_form"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='order_line']/tree/field[last()]" position="after">
                <field name="profit_percent" string="% Util." readonly="1"/>
            </xpath>
        </field>
    </record>
</odoo>
