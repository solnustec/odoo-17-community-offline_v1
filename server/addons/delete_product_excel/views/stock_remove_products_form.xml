<odoo>
    <record id="view_stock_remove_products_form" model="ir.ui.view">
        <field name="name">stock.remove.products.form</field>
        <field name="model">stock.remove.products</field>
        <field name="arch" type="xml">
            <form string="Eliminar productos por Excel">
                <group>
                    <field name="file" widget="binary"/>
                    <field name="filename"/>
                </group>
                <footer>
                    <button name="action_process_file" type="object" string="Procesar" class="btn-primary"/>
                    <button string="Cancelar" class="btn-secondary" special="cancel"/>
                </footer>
            </form>
        </field>
    </record>
</odoo>
