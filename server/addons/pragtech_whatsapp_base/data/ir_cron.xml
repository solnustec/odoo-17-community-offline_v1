<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="ir_cron_check_inactivity_chatbot" model="ir.cron">
        <field name="name">Verificar Inactividad de Chatbot</field>
        <field name="model_id" ref="model_whatsapp_chatbot"/>
        <field name="state">code</field>
        <field name="code">model._cron_check_inactivity()</field>
        <field name="interval_number">1</field>
        <field name="interval_type">minutes</field>
        <field name="numbercall">-1</field>
        <field name="active" eval="True"/>
        <field name="user_id" ref="base.user_root"/>
    </record>
</odoo>