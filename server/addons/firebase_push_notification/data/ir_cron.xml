<odoo>
    <record id="ir_cron_push_notification_sender" model="ir.cron">
        <field name="name">Enviar Notificaciones Push</field>
        <field name="model_id" ref="model_push_message_line"/>
        <field name="state">code</field>
        <field name="code">model.send_pending_notifications()</field>
        <field name="interval_number">10</field>
        <field name="interval_type">minutes</field>
        <field name="numbercall">-1</field>
        <field name="active">True</field>
    </record>
</odoo>