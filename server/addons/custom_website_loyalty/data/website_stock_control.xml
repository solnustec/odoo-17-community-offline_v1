<odoo>
    <data noupdate="1">

        <record id="ir_cron_hide_out_of_stock_products" model="ir.cron">
            <field name="name">Ocultar Productos sin stock del ecommerce</field>
            <field name="model_id" ref="model_product_template"/>
            <field name="state">code</field>
            <field name="code">model.action_hide_out_of_stock_products()</field>
            <field name="interval_number">5</field>
            <field name="interval_type">minutes</field>
            <field name="numbercall">-1</field>
            <field name="active">True</field>
        </record>
    </data>
</odoo>
