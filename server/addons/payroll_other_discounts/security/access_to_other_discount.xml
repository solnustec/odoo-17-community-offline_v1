<odoo>
    <data noupdate="1">
         <record model="ir.module.category" id="hr_payroll_discount_category">
            <field name="name">Abonos y descuentos</field>
            <field name="description">Tiene acceso al calculo de utilidades</field>
            <field name="sequence">30</field>
        </record>
        <record id="group_hr_payroll_discount_admin" model="res.groups">
            <field name="name">Admin Abonos y Descuentos</field>
            <field name="category_id" ref="hr_payroll_discount_category"/>
            <field name="users" eval="[(4, ref('base.user_root')), (4, ref('base.user_admin'))]"/>
        </record>
    </data>
</odoo>
