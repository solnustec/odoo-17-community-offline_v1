<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="view_warehouse_orderpoint_tree_editable_custom" model="ir.ui.view">
        <field name="name">stock.warehouse.orderpoint.custom.tree</field>
        <field name="model">stock.warehouse.orderpoint</field>
        <field name="inherit_id" ref="stock.view_warehouse_orderpoint_tree_editable"/>
        <field name="arch" type="xml">
            <!-- Insertar point_reorder antes de product_max_qty -->
            <xpath expr="//field[@name='product_max_qty']" position="before">
                <field name="point_reorder" optional="hide"/>
            </xpath>
        </field>
    </record>
</odoo>