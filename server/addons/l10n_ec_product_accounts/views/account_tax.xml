<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="view_tax_form_inhenrit" model="ir.ui.view">
        <field name="name">account.tax.form.inherit</field>
        <field name="model">account.tax</field>
        <field name="inherit_id" ref="account.view_tax_form"/>
        <field name="arch" type="xml">
            <xpath expr="//notebook//page[@name='advanced_options']" position="after">
                <page string="Contabilidad" name="accounting_options">
                    <group>
                        <group>
                            <field name="inventory_income_account_id"/>
                            <field name="inventory_expense_account_id"/>
                            <field name="inventory_stock_valuation_account_id"/>
                        </group>
                    </group>
                </page>
            </xpath>
        </field>
    </record>
</odoo>